<html>
    
    <% include ../partials/head %>
    
    <body>
        <header>
            <% include ../partials/header %>
        </header>
        <div id="output"></div>
        <table class="table table-striped table-dark">
                <thead>
                  <tr>
                    <th>Id</th>
                    <th>Username</th>
                  </tr>
                </thead>
                <tbody>

                  <% for(var i=0; i < users.length; i++) { %>
                    <tr>
                        <td><%= users[i].id %></td>
                        <td><%= users[i].username %></td>                        
                    </tr>
                  <% } %>
                 
                </tbody>
              </table>
    </body>
</html>

