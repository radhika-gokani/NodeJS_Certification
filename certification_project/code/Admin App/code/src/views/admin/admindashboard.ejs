<html>
    
    <% include ../partials/head %>
    
    <body>
        <header>
            <% include ../partials/header %>
        </header>
        <div id="output"></div>
        <table class="table table-striped table-dark">
                <thead>
                  <tr>
                    <th>Title</th>
                    <th>Description</th>
                    <th>Published At</th>
                    <th>Actions</th>
                  </tr>
                </thead>
                <tbody>

                  <% for(var i=0; i < news.length; i++) { %>
                    <tr>
                        <td><%= news[i].title %></td>
                        <td><%= news[i].description %></td>
                        <td><%= news[i].createdAt %></td>
                        <td>Edit/delete</td>                                            
                    </tr>
                  <% } %>
                 
                </tbody>
              </table>
    </body>
</html>

