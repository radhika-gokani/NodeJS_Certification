  <% include ./partials/header %>
              <!-- wheather section -->
              <div class="row mt-3 p-3 weather">
                    <div class="col-sm-3">
                      <div class="card">
                          <img src="data:image/png;base64," class="card-img-top" alt="weather icon">
                        <div class="card-body">
                          <h5 class="card-title">Weather Forecast</h5>
                          <p class="card-text">Today's weather</p>
                        </div>
                      </div>
                    </div>
                    <div class="col-sm-3">                        
                      <div class="card">
                          <img id="weatherIcon" class="card-img-top" alt="loading...">
                        <div class="card-body">
                          <h5 class="card-title">Weather</h5>
                          <p id="weatherText" class="card-text"></p>
                        </div>
                      </div>
                    </div>
                    <div class="col-sm-3">                        
                            <div class="card">
                              <img src="data:image/png;base64," class="card-img-top" alt="...">
                              <div class="card-body">
                                <h5 class="card-title">Temprature</h5>
                                <p id="weatherTemp" class="card-text">Temprature is</p>

                              </div>
                            </div>
                          </div>

                          <div class="col-sm-3">
                                <div class="card">
                                  <img src="data:image/png;base64," class="card-img-top" alt="...">
                                  <div class="card-body">
                                    <h5 class="card-title">Location</h5>
                                    <p id="weatherLocation" class="card-text">Your location is</p>
                                  </div>
                                </div>
                              </div>
                  </div>
              <!-- slider -->
              
             
                  <div id="carouselExampleIndicators" class="carousel slide" data-ride="carousel" style="width: 90%;margin:auto">
                      <ol class="carousel-indicators">
                        <li data-target="#carouselExampleIndicators" data-slide-to="0" class="active"></li>
                        <li data-target="#carouselExampleIndicators" data-slide-to="1"></li>
                        <li data-target="#carouselExampleIndicators" data-slide-to="2"></li>
                      </ol>
                      <div class="carousel-inner">
                        <div class="carousel-item active">
                          <img src="https://encrypted-tbn0.gstatic.com/images?q=tbn:ANd9GcSTbpBwv7Ija2vQpVfb2dDVf7qdKW8kY0FE0Gbw6FFx59W81Fgssw" class="d-block w-100" alt="...">
                        </div>
                        <div class="carousel-item">
                          <img src="https://image.shutterstock.com/image-vector/creative-vector-illustration-world-live-260nw-1106986307.jpg" class="d-block w-100" alt="...">
                        </div>
                        <div class="carousel-item">
                          <img src="https://a57.foxnews.com/media2.foxnews.com/BrightCove/694940094001/2019/05/06/931/524/694940094001_6033508755001_6025576909001-vs.jpg?ve=1&tl=1" class="d-block w-100" alt="...">                             
                      </div>
                      
                    </div>
              </div>
              <br>
              <br>
              <br>
              <!-- latest news -->
              <center><h1>Latest News</h1></center>
              <div class="row mt-3 p-3 latestnews" >
              <% for(var i=0; i < latestnews.length; i++) { %>
                <div class="col-sm-3">
                  <div class="card">
                      <img src="<%= latestnews[i].imageurl %>" class="card-img-top" alt="weather icon">
                    <div class="card-body">
                      <h5 class="card-title"><%= latestnews[i].title %></h5>
                      <p id="weatherLocation" class="card-text"><%= latestnews[i].description %></p>
                    </div>
                  </div>
                </div>
              <% } %>
